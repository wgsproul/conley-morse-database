<config>
<model>
  <name>Newton Model </name>
  <desc> Exploring the limit of iterates in newton convergence. </desc>
</model>
<param>
  <dim> 4 </dim>
  <bounds>
    <lower> -1.0 -1.0 -1.0 -3.142</lower>
    <upper> 1.0 1.0 1.0 3.142</upper>
  </bounds>
  <subdiv>
    <depth> 4 </depth>
  </subdiv>
</param>
<phase>
  <dim> 1 </dim>
  <bounds>
    <lower> -3.14159265358979323846264338327950288 </lower>
    <upper> 3.14159265358979323846264338327950288 </upper>
  </bounds>
  <subdiv>
    <min> 10 </min>
    <max> 15 </max>
    <limit> 10000 </limit>
  </subdiv>
  <periodic> 1 </periodic>
</phase>
</config>

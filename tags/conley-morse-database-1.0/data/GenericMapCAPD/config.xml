<config> <!-- Beginning of the configuration -->
<!-- Set up the name and the description of the model -->
<model>
  <name> Leslie2D </name>
  <desc> Two dimensional Leslie model </desc>
</model>
<!-- Set up the parameter space -->
<param>
  <dim> 2 </dim> <!-- Dimension of the parameter space -->
  <bounds>
    <lower> 26 22.7 </lower>  <!-- Lower bounds of the parameter space (order is important) -->
    <upper> 26 22.7 </upper><!-- Upper bounds of the parameter space (order is important) -->
  </bounds>
  <subdiv>
    <depth> 5 </depth> <!-- number of subdivisions in each direction of the parameter space -->
  </subdiv>
</param>
<!-- Set up the phase space -->
<phase>
  <dim> 2 </dim> <!-- Dimension of the phase space -->
  <bounds>
    <lower> 0.0 0.0 </lower>  <!-- Lower bounds of the phase space (order is important) -->
    <upper> 320.056 224.040 </upper> <!-- Upper bounds of the phase space (order is important) -->
  </bounds>
  <subdiv>
    <init> 0 </init> <!-- Initial number of subdivisions -->
    <min> 26 </min> <!-- Minimum number of subdivisions -->
    <max> 28 </max> <!-- Maximum number of subdivisions -->
    <limit> 1000 </limit> <!--  -->
  </subdiv>
</phase>
</config> <!-- End of the configuration -->
